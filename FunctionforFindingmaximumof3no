#include <iostream>
using namespace std;
int maximum(int x, int y, int z)
{
	if(x > y && x > z)
	return x;
	else if (y > x && y > z)
	return y;
	else 
	return z;
}

int main()
{
	int a, b, c, max;
	cout << "Enter the first number: ";
	cin >> a;
	cout << "Enter the second number : ";
	cin >> b;
	cout << "Enter the third number : ";
	cin >> c;
	max= maximum(a,b,c);
	cout << "Maximum number is : " << max;
	return 0;
}
