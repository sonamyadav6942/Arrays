--------------------------------------------------------------------------------------------------------------------------------------------------------------------------

#include<iostream>
using namespace std;
int add(int x, int y)
{
    int z;
    z = x + y;
    return z;
}
void Main()
{
    int a = 10, b = 5, c;
    c = add(a,b);
    cout << "Sum of two numbers is : " << c;
}

--------------------------------------------------------------------------------------------------------------------------------------------------------------------------

#include <iostream>
using namespace std;
int add(int x, int y)
{
	int z;
	z = x + y;
	return z;
}

int main()
{
	int a, b, sum;
	cout << "Enter the first value : ";
	cin >> a;
	cout << "Enter the second value : ";
	cin >> b;
	sum = add(a,b);
	cout << "Sum of two numbers is : " << sum;
  }
  
  ------------------------------------------------------------------------------------------------------------------------------------------------------------------------
